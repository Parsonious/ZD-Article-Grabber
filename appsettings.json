{
  "Paths": {
    "HtmlFilesPath": "/api/static-files/html",
    "ResourceFilesPath": "/api/static-files/",
    "UrlPath": "https://parsonious.github.io/How-To/pages/",
    "DefaultFiles": {
      "css": "default.css",
      "js": "default.js",
      "img": "default.png",
      "sql": "default.sql",
      "ps1": "default.ps1"
    },
    "SupportedFileTypes": [ "css", "js", "img", "sql", "ps1" ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:80"
      },
      "Https": {
        "Url": "https://0.0.0.0:443",
        "Certificate": {
          "Path": "/etc/letsencrypt/live/bepio.net/fullchain.pem",
          "KeyPath": "/etc/letsencrypt/live/bepio.net/privkey.pem"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
