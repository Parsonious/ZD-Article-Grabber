{
  "Paths": {
    "HtmlFilesPath": "/api/source/html",
    "ResourceFilesPath": "/api/source/",
    "FallbackRemoteUrlPath": "https://parsonious.github.io/How-To/pages/",
    "ExternalResourceUrlPath": "https://bepio.net/api/source/"
  },
  "Files": {
    "SupportedFileTypes": [ "html", "css", "js", "img", "sql", "ps1" ],
    "DefaultFiles": {
      "html": "default.html",
      "css": "default.css",
      "js": "default.js",
      "img": "default.png",
      "sql": "default.sql",
      "ps1": "default.ps1",
      "tsql": "default.tsql"
    }
  },
  "Referer": {
    "AllowedDomains": [ "dev.bepio.net", "compiq.zendesk.com", "bepio.net" ],
    "EnforceHttps": true
  },
  "XPathQueries": {
    "//link[@rel='stylesheet' and @href]": "css",
    "//script[@src]": "js",
    "//img[@src]": "img",
    "//a[@href][contains(@href, '.sql')]": "sql",
    "//a[@href][contains(@href, '.ps1')]": "ps1"
  },
  "Jwt": {
    "TokenKey": "${JWT_TOKEN_KEY}",
    "Issuer": "https://bepio.net",
    "ApiKey": "${JWT_API_KEY}",
    "ExpirationInMinutes": 10
  },
  "KeyManagement": {
    "KeyActiveFolder": "/app/keys/active",
    "KeyArchiveFolder": "/app/keys/archive",
    "RotationIntervalDays": 30,
    "KeyLifetimeDays": 90,
    "RetentionPeriodDays": 180,
    "MinimumKeyAge": 2,
    "WarningThresholdDays": 7
  },
  "DomainClaims": {
    "Settings": {
      "compiq.zendesk.com": {
        "Claims": [
          {
            "Type": "HTML",
            "Value": "PlainText"
          },
          {
            "Type": "JS",
            "Value": "PlainText"
          },
          {
            "Type": "CSS",
            "Value": "PlainText"
          },
          {
            "Type": "SQL",
            "Value": "PlainText"
          },
          {
            "Type": "PS1",
            "Value": "PlainText"
          }
        ]
      },
      "dev.bepio.net": {
        "Claims": [
          {
            "Type": "HTML",
            "Value": "PlainText"
          },
          {
            "Type": "JS",
            "Value": "PlainText"
          },
          {
            "Type": "CSS",
            "Value": "PlainText"
          },
          {
            "Type": "SQL",
            "Value": "PlainText"
          },
          {
            "Type": "PS1",
            "Value": "PlainText"
          }
        ]
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
